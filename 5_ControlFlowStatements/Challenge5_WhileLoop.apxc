// Print first 20 fibonacci numbers

// Variables
Integer num1 = 0;
Integer num2 = 1;
Integer sum;
Integer count = 0;

// Print the fist two numbers
System.debug(num1);
System.debug(num2);

// Repeat until it has print the 20 numbers
while (count < 18) {
    // Print the new number
    sum = num1 + num2;
    System.debug(sum);

    // Change both variables
    num1 = num2;
    num2 = sum;

    // Increases in one
    count++;
}